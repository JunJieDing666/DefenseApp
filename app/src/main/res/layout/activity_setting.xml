<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        style="@style/TitleStyle"
        android:text="设置中心" />

    <!--维护相应自定义属性的名空间-->
    <com.jj.defense.View.SettingItemView xmlns:defense="http://schemas.android.com/apk/res/com.jj.defense"
        android:id="@+id/siv_update"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        defense:desoff="    自动更新已关闭"
        defense:deson="    自动更新已开启"
        defense:titledes="自动更新设置:"></com.jj.defense.View.SettingItemView>

    <com.jj.defense.View.SettingItemView xmlns:defense="http://schemas.android.com/apk/res/com.jj.defense"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        defense:desoff="    归属地显示已关闭"
        defense:deson="    归属地显示已开启"
        defense:titledes="电话归属地显示设置:"></com.jj.defense.View.SettingItemView>+

</LinearLayout>
